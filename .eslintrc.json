{
    "extends": "eslint-config-airbnb",
    "rules": {
        "indent": 0,
        "max-len": ["error", 160],
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-mixed-operators": 0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "no-unused-vars": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 0,
        "prefer-template": 1,
        "no-lonely-if": 0,
        "yoda": ["error", "never"],
        "space-before-function-paren": 0,
        "eol-last": 0
    }
}